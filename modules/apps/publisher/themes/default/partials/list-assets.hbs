<div class="asset-being-added">
</div>
<div class="asset-thumb-container">
    {{#if assets}}
        <div class="row-fluid">
            <table class="table table-striped list-asset-table">
                <thead>
                <th colspan="2"><a href="?sortby=overview_name&sort=ASC">Name <i class="icon-caret-down"></i></a></th>
                <th><a href="?sortby=overview_version&sort=ASC">Version <i class="icon-caret-down"></i></a></th>
                <th><a href="?sortby=overview_provider&sort=ASC">Owner <i class="icon-caret-down"></i></a></th>
                <th><a href="#">Status</i></a></th>
                <th><a href="?sortby=overview_createdtime&sort=DESC">Created <i class="icon-caret-down"></i></a></th>
                <th></th>
                </thead>
                <tbody id='list-asset-table-body'>
                    
                    {{> list-assets-table-body assets}}
                 
                </tbody>
            </table>
        </div>
    {{else}}
            <div class="emptyAssets-MsgDiv">
                <i class="icon-frown largeMsgIcon"></i>
                
                 <p class="emptyAssets">Sorry. I couldn't find anything for you</p>
                
            </div>
    {{/if}}
</div>
